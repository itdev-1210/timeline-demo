{
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "."
                ],
                "alias": {
                    "styles": "./styles"
                },
                "cwd": "babelrc"
            }
        ],
        [
            "wrap-in-js",
            {
                "extensions": [
                    "css$",
                    "scss$"
                ]
            }
        ],
        [
            "transform-define",
            "./env-config.js",
            "transform-decorators-legacy"
        ]
    ],
    "presets": [
        "next/babel"
    ],
    "ignore": []
}